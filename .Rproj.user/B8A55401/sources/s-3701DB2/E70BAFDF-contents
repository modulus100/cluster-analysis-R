m <- matrix(1:9, nrow = 3, ncol = 3)

# For each row and for each column, assign values based on position: product of two indexes
for(i in 1:dim(m)[1]) {
  for(j in 1:dim(m)[2]) {
    #m[i,j] = i*j
    print(m[i,j])
  }
}
